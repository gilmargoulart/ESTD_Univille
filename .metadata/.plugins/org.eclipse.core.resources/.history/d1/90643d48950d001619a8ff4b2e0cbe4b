package exercicios;

import org.junit.Assert;
import org.junit.Test;

public class Ex1AnagramaTest {
	
	@Test
	public void testeAnagrama(){
		
		//Definir palavra 1
		String str1 = "Gato";
		//Definir palavra 2
		String str2 = "Toga";
		
		Ex1Anagrama anagrama = new Ex1Anagrama(str1, str2);
		
		Assert.assertTrue(anagrama.isAnagrama());
		
	}
	
}
